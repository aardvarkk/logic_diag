grammar Grammar
  rule commandset
    command+
  end

  rule command
    identifier command_detail? comment_to_eol? "\n"
  end

  rule command_detail
    assign_expr / expr
  end

  rule assign_expr
    identifier ":=" expr
  end

  rule expr
    [^#\n]*
  end

  rule identifier
    [a-zA-Z0-9]+ ws+
  end

  rule comment_to_eol
    "#" [^\n]*
  end

  rule ws
    [ ]
  end
end